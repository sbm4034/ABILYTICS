<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Wipocab.abilytics.application">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name="com.Wipocab.abilytics.application.MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name="com.Wipocab.abilytics.application.SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.Wipocab.abilytics.application.Profile"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.Wipocab.abilytics.application.VerifyOtpActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.Wipocab.abilytics.application.ActProductInfo"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.Wipocab.abilytics.application.Activity_cart"
            android:screenOrientation="portrait" />
    </application>

</manifest>